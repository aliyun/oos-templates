  "RamRole": "{{ OOSAssumeRole }}",
  "Tasks": [
    {
      "Name": "getResourceARN",
      "Action": "ACS::ExecuteAPI",
      "Description": {
        "en": "Get ResourcesArn with specified tag",
        "zh-cn": "获取带有指定标签的ResourcesArn"
      },
      "Properties": {
        "Service": "TAG",
        "API": "ListTagResources",
        "Parameters": {
          "RegionId": "{{ regionId }}",
          "Tags": "{{ oldTag }}",
          "ResourceARN": [
            {
              "Fn::If": [
                {
                  "Fn::Equals": [
                    "{{ resourceType }}",
                    ""
                  ]
                },
                null,
                {
                  "Fn::Jq": [
                    "First",
                    ".resourceType | ascii_downcase |split(\"::\") | \"arn:acs:\"+.[1]+\":{{ regionId }}:{{ ACS::AccountId }}:\"+.[2]+\"/*\"",
                    "{\"resourceType\":\"{{resourceType}}\"}"
                  ]
                }
              ]
            }
          ]
        }
      },
      "Outputs": {
        "resourceArns": {
          "Type": "List",
          "ValueSelector": "TagResources[].ResourceARN"
        }
      }
    },
    {
      "Name": "unTagResourcesIfNecessary",
      "Action": "ACS::ExecuteAPI",
      "When": {
        "Fn::Not": {
          "Fn::Equals": [
            {
              "Fn::Jq": [
                "First",
                ".[].Key",
                {
                  "Fn::ConvertMapToList": [
                    "{{ oldTag }}"
                  ]
                }
              ]
            },
            {
              "Fn::Jq": [
                "First",
                ".[].Key",
                {
                  "Fn::ConvertMapToList": [
                    "{{ newTag }}"
                  ]
                }
              ]
            }
          ]
        }
      },
      "Description": {
        "en": "Delete old tag when the old and new tag key are inconsistent",
        "zh-cn": "当旧标签键与新标签键不一致时删除旧标签"
      },
      "Properties": {
        "Service": "TAG",
        "API": "UnTagResources",
        "Parameters": {
          "RegionId": "{{ regionId }}",
          "TagKey": [
            {
              "Fn::Jq": [
                "First",
                ".[].Key",
                {
                  "Fn::ConvertMapToList": [
                    "{{ oldTag }}"
                  ]
                }
              ]
            }
          ],
          "ResourceARN": [
            "{{ACS::TaskLoopItem}}"
          ]
        }
      },
      "Outputs": {
        "failedResource": {
          "Type": "List",
          "ValueSelector": "FailedResources.FailedResource[]"
        }
      },
      "Loop": {
        "RateControl": "{{rateControl}}",
        "Items": "{{getResourceARN.resourceArns}}",
        "Outputs": {
          "untagResult": {
            "AggregateType": "Fn::ListJoin",
            "AggregateField": "failedResource"
          }
        }
      }
    },
    {
      "Name": "tagResource",
      "Action": "ACS::ExecuteApi",
      "Description": {
        "en": "Tag resources with new tag through resource management",
        "zh-cn": "通过资源管理给资源打上新的标签"
      },
      "Properties": {
        "Service": "TAG",
        "API": "TagResources",
        "Parameters": {
          "RegionId": "{{ regionId }}",
          "ResourceARN": [
            "{{ACS::TaskLoopItem}}"
          ],
          "Tags": "{{ newTag }}"
        }
      },
      "Outputs": {
        "failedResource": {
          "Type": "List",
          "ValueSelector": "FailedResources.FailedResource[]"
        }
      },
      "Loop": {
        "RateControl": "{{rateControl}}",
        "Items": "{{getResourceARN.resourceArns}}",
        "Outputs": {
          "tagResult": {
            "AggregateType": "Fn::ListJoin",
            "AggregateField": "failedResource"
          }
        }
      }
    }
  ],
  "Outputs": {
    "resourceArns": {
      "Type": "List",
      "Value": "{{ getResourceARN.resourceArns }}"
    },
    "untagfailedResource": {
      "Type": "List",
      "Value": "{{ unTagResourcesIfNecessary.untagResult }}"
    },
    "tagfailedResource": {
