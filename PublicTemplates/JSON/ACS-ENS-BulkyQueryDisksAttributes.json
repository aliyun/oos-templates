{
  "FormatVersion": "OOS-2019-06-01",
  "Description": {
    "en": "Bulky query disk ID and disk information",
    "zh-cn": "批量查询磁盘ID及磁盘信息",
    "name-en": "ACS-ENS-BulkyQueryDisksAttributes",
    "name-zh-cn": "批量查询实例ID及磁盘信息"
  },
  "Parameters": {
    "rateControl": {
      "Label": {
        "en": "RateControl",
        "zh-cn": "任务执行的并发比率"
      },
      "Type": "Json",
      "AssociationProperty": "RateControl",
      "Default": {
        "Mode": "Concurrency",
        "MaxErrors": 0,
        "Concurrency": 10
      }
    },
    "OOSAssumeRole": {
      "Label": {
        "en": "OOSAssumeRole",
        "zh-cn": "OOS扮演的RAM角色"
      },
      "Type": "String",
      "Default": ""
    }
  },
  "RamRole": "{{ OOSAssumeRole }}",
  "Tasks": [
    {
      "Name": "ExecuteApi",
      "Action": "ACS::ExecuteApi",
      "Description": "查询ENS磁盘列表。",
      "Properties": {
        "Parameters": {},
        "Service": "ens",
        "API": "DescribeDisks"
      },
      "Outputs": {
        "DisksInfo": {
          "Type": "List",
          "ValueSelector": ".Disks.Disks[]"
        }
      }
    }
  ],
  "Outputs": {
    "disks_info": {
      "Type": "List",
      "Value": "{{ ExecuteApi.DisksInfo }}"
    }
  }
}